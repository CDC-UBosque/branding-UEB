						+ejemplos('../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-tinta-negra.svg', 'Logotipo Universidad El Bosque horizontal a una linea', 'classimg', 'Horizontal a una línea positivo a una tinta', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-tinta-negra.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-tinta-negra.png')
						
						+ejemplos('../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-invertido.svg', 'Logotipo Universidad El Bosque horizontal a una linea', 'classimg invertido', 'Horizontal a una línea', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-invertido.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-invertido.png')
						
						//- vertical a una linea
						+ejemplos('../descargas/logo-ueb/logo-ueb-vertical-a-una-linea.svg', 'Logotipo Universidad El Bosque vertical a una linea', 'classimg', 'Vertical a una línea', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea.png')
						
						+ejemplos('../descargas/logo-ueb/logo-ueb-vertical-a-una-linea-tinta-negra.svg', 'Logotipo Universidad El Bosque vertical a una linea', 'classimg', 'Vertical a una línea positivo a una tinta', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-tinta-negra.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-tinta-negra.png')
						
						+ejemplos('../descargas/logo-ueb/logo-ueb-vertical-a-una-linea-invertido.svg', 'Logotipo Universidad El Bosque vertical a una linea', 'classimg invertido', 'Vertical a una línea', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-invertido.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-una-linea-invertido.png')
						
						//- vertical a dos lineas
						+ejemplos('../descargas/logo-ueb/logo-ueb-vertical-a-dos-lineas.svg', 'Logotipo Universidad El Bosque vertical a una linea', 'classimg', 'Vertical a dos líneas', '../descargas/logo-ueb/logo-ueb-horizontal-a-dos-lineas.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-dos-lineas.png')
						
						+ejemplos('../descargas/logo-ueb/logo-ueb-vertical-a-dos-lineas-tinta-negra.svg', 'Logotipo Universidad El Bosque vertical a una linea', 'classimg', 'Vertical a dos líneas positivo a una tinta', '../descargas/logo-ueb/logo-ueb-horizontal-a-dos-lineas-tinta-negra.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-dos-lineas-tinta-negra.png')
						
						+ejemplos('../descargas/logo-ueb/logo-ueb-vertical-a-dos-lineas-invertido.svg', 'Logotipo Universidad El Bosque vertical a una linea', 'classimg invertido', 'Vertical a dos líneas', '../descargas/logo-ueb/logo-ueb-horizontal-a-dos-lineas-invertido.svg', '../descargas/logo-ueb/logo-ueb-horizontal-a-dos-lineas-invertido.png')
						
					h2 Logotipo Universidad El Bosque - Vigilada Mineducación
					
					p A continuación puede encontrar nuestro logo con el legal Vigilada Mineducación, por favor úselo responsablemente.
						
						
						
						
						
			